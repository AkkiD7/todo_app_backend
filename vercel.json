{
  "version": 2,
  "builds": [
    {
      "src": "index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/todos/(.*)", 
      "dest": "index.js"  
    },
    {
      "handle": "filesystem" 
    },
    {
      "src": "/(.*)", 
      "status": 404,
      "dest": "index.js"
    }
  ]
}
